{
  "name": "rss-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup.mjs",
    "db:init:auth": "node scripts/db-init-auth.mjs",
    "dev": "next dev -p 3000",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "dev:server": "tsx watch server/src/index.ts",
    "dev:web": "next dev -p 3000",
    "build:server": "tsc -p server/tsconfig.json",
    "prestart:server": "npm run build:server",
    "start:server": "node server/dist/index.js",
    "test": "node --test tests/auth.test.mjs",
    "smoke": "node scripts/smoke.mjs",
    "import:default:feeds": "node scripts/import-default-feeds-via-api.mjs",
    "demo:supabase:feeds": "node scripts/supabase-feeds-demo.mjs",
    "validate": "node scripts/validate-env.mjs",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "wrangler:env": "node scripts/sync-wrangler-env.mjs",
    "build:cf": "npm run wrangler:env && npx @opennextjs/cloudflare@latest build",
    "preview:cf": "npm run build:cf && npx wrangler@latest dev --env-file .env.wrangler",
    "deploy:cf": "npm run build:cf && npx wrangler@latest deploy --env-file .env.wrangler"
  },
  "dependencies": {
    "@langchain/langgraph": "^0.2.55",
    "@mozilla/readability": "^0.6.0",
    "@supabase/supabase-js": "^2.49.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsdom": "^26.1.0",
    "next": "^16.1.6",
    "playwright": "^1.49.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.39",
    "tailwindcss": "^4.1.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
